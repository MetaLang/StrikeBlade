        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Shock Bot States

// Check for certain dealth
if(hp &lt; 1) {
 currentState = ShockBotStates.exploding;
}


switch(currentState) { 
    case ShockBotStates.patrolling:
    
        if((abs(objPlayer.y - y) &lt;= 40) &amp;&amp; (abs(objPlayer.x - x) &lt; viewField)) {
            currentState = ShockBotStates.attacking;
        
        }
        
        sprite_index = sprShockBotRun;
        
        if(image_xscale &lt; 0) { // Move Left
           x -= runSpeed; 
        }
        else { // Move Right
            x += runSpeed;
        }
        
        // If close to an edge
        if(!position_meeting(x, y+(sprite_height/2)+8, objPlatform)) {
            image_xscale *= -1;
        }
            
    break;
    
    case ShockBotStates.attacking:
    
        if((abs(objPlayer.y - y) &gt; 40) || (abs(objPlayer.x - x) &gt;= viewField)) {
            currentState = ShockBotStates.patrolling;
            break;
        }
    
        // Check if Hero is on left or right

        if(objPlayer.x &lt; x) {  // Left
            image_xscale = -1;
             x -= runSpeed; 
        }
        else {  // Right
            image_xscale = 1;
            x += runSpeed;
        }
        
        if(distance_to_object(objPlayer) &lt; viewField) {
            sprite_index = sprShockBotAttack;
        }
        else {
            sprite_index = sprShockBotRun;
        }
        

    
    break;
    
    case ShockBotStates.bouncingBack:
        sprite_index = sprShockBotCoolDown;
        speed = runSpeed; 
        
        // If close to an edge
        if(!position_meeting(x + (20 * -image_xscale), y+(sprite_height/2)+8, objPlatform)) {
            currentState = ShockBotStates.coolingDown;
        }
        
    break;
        
    case ShockBotStates.coolingDown:
        sprite_index = sprShockBotCoolDown;
        speed = 0;
        runSpeed = 5;
    break;
        
    case ShockBotStates.exploding:
        // Create Explosion
        audio_play_sound(sndExplosion2, 10, false);
        effect_create_above(ef_explosion, x, y, 1, c_blue);
        effect_create_above(ef_firework, x, y, 1, c_yellow);
        var scorePopUp = instance_create(x, y, objScore);
        scorePopUp.pointsValue = pointsValue;
        
        spawnChance = random(10);

        if(spawnChance &lt; 1) {
             instance_create(x, y, objArmourPickup);
        }
        
        instance_destroy();
    break; 
}

    
</string>
